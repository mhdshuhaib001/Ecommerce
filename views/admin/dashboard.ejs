  <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>



  <script>
    var paymentMethod = document.getElementById('paymentMethod');
    var paymentMethodCount = JSON.parse(paymentMethod.getAttribute('data-payment-method'));
    var methodLabels = paymentMethodCount.map(method => method._id);
    var methodData = paymentMethodCount.map(method => method.count);

    console.log(methodLabels, 'chekk this method labels');
    console.log(methodData, 'chekk this method data');

    // Your data object
    const data = {
      labels: methodLabels,
      datasets: [{
        label: 'Payment Methods',
        data: methodData,
        backgroundColor: [
          'rgba(255, 99, 132, 0.6)', // Light Red
          'rgba(54, 162, 235, 0.6)', // Light Blue
          'rgba(255, 205, 86, 0.6)', // Light Yellow
          'rgba(75, 192, 192, 0.6)', // Light Teal
          'rgba(153, 102, 255, 0.6)', // Light Purple
        ],
        hoverOffset: 4
      }]
    };



    const config = {
      type: 'pie',
      data: data,
    };

    const canvas = document.getElementById('transactionChart');

    const myPieChart = new Chart(canvas, config);




    //---------------------------------sales chart----------------------------------------------
    // monthlyData
    var monthlySales = document.getElementById('monthlySales');
    var salesDataMonthly = JSON.parse(monthlySales.getAttribute('data-monthly-order-counts'));
    const monthlyLabels = ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'];

    // yearlyData
    var yearlySales = document.getElementById('yearlySales');
    var salesDataYearly = JSON.parse(yearlySales.getAttribute('data-yearly-order-counts'));

    const yearlyLabels = ['2023', '2024', '2025', '2026',];

    const monthlyData = {
      labels: monthlyLabels,
      datasets: [{
        label: 'Monthly Dataset',
        data: salesDataMonthly,
        backgroundColor: 'rgba(255, 99, 132, 0.2)',
        borderColor: 'rgb(255, 99, 132)',
        borderWidth: 1
      }]
    };

    const yearlyData = {
      labels: yearlyLabels,
      datasets: [{
        label: 'Yearly Dataset',
        data: salesDataYearly,
        backgroundColor: 'rgba(75, 192, 192, 0.2)',
        borderColor: 'rgb(75, 192, 192)',
        borderWidth: 1
      }]
    };

    // Default configuration with dynamic monthly data
    const initialConfig = {
      type: 'bar',
      data: monthlyData,
      options: {
        scales: {
          y: {
            beginAtZero: true
          }
        }
      }
    };


    const ctx = document.getElementById('salesChart').getContext('2d');
    const chart = new Chart(ctx, initialConfig);

    function toggleDataset() {
      const currentData = chart.data;
      if (currentData.datasets[0].label === 'Monthly Dataset') {
        chart.data = yearlyData;
      } else {
        chart.data = monthlyData;
      }
      // Update the button text
      const button = document.getElementById('toggleButton');
      button.innerText = currentData.datasets[0].label === 'Monthly Dataset' ? 'Yearly Data' : ' Monthly Data';
      chart.update();
    }



    // ------------------------------------bestSellingProduct-------------------------
    var bestSellingProducts = document.getElementById('bestSellingProducts');
    var bestSellingProduct = JSON.parse(bestSellingProducts.getAttribute('data-best-seller'));
    const bestProduct = bestSellingProduct.map(product => product.productName);
    const bestSellingProductCount = bestSellingProduct.map(product => product.totalSold);

    const dataBestsellingProduct = {
      labels: bestProduct,
      datasets: [{
        axis: 'y',
        label: 'Best Selling Product',
        data: bestSellingProductCount,
        backgroundColor: [
          'rgba(255, 99, 132, 0.2)',
          'rgba(255, 159, 64, 0.2)',
          'rgba(255, 205, 86, 0.2)',
          'rgba(75, 192, 192, 0.2)',
          'rgba(54, 162, 235, 0.2)',
          'rgba(153, 102, 255, 0.2)',
          'rgba(201, 203, 207, 0.2)'
        ],
        borderColor: [
          'rgb(255, 99, 132)',
          'rgb(255, 159, 64)',
          'rgb(255, 205, 86)',
          'rgb(75, 192, 192)',
          'rgb(54, 162, 235)',
          'rgb(153, 102, 255)',
          'rgb(201, 203, 207)'
        ],
        borderWidth: 1
      }]
    };

    const configBestsellingProduct = {
      type: 'bar',
      data: dataBestsellingProduct,
      options: {
        indexAxis: 'y',
      }
    };

    const bestsellerPro = document.getElementById('bestSellingProductChart').getContext('2d');
    const myChart = new Chart(bestsellerPro, configBestsellingProduct);





  </script>

