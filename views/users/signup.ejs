    <body>

        <div class="login">
            <img src="img/slider/12.jpg" alt="image" class="login__bg">
            <!-- autocomplete is of -->
            <form action method="post" id="form" class="login__form">
                <h1 class="login__title">Sign Up</h1>

                <div class="login__inputs">
                    <div class="login__box">
                        <input type="text" placeholder="Enter your Full Name "
                            name="name" id="name"
                            class="login__input">
                    </div>
                    <div class="error-message" id="name-error"
                        style="  display: none;
                        color: rgb(161 17 17);
                        font-weight: bold;
                        font-size: 12px; "></div>

                    <div class="login__box">
                        <input type="email" placeholder="Email ID" name="email"
                            id="email" class="login__input">
                        <i class="ri-mail-fill"></i>
                    </div>
                    <div class="error-message" id="email-error"
                        style="  display: none;
                    color: rgb(161 17 17);
                    font-weight: bold;
                    font-size: 12px; "></div>

                    <div class="login__box">
                        <input type="tel" placeholder="Phone" name="mobile"
                            id="mobile" class="login__input">
                    </div>
                    <div class="error-message" id="phone-error"
                        style="  display: none;
                        color: rgb(161 17 17);
                        font-weight: bold;
                        font-size: 12px; "></div>

                    <div class="login__box">
                        <input type="password" placeholder="Password"
                            name="password" id="password"
                            class="login__input">
                        <i class="ri-lock-2-fill"></i>
                    </div>
                    <div class="error-message" id="password-error"
                        style="  display: none;
                    color: rgb(161 17 17);
                    font-weight: bold;
                    font-size: 12px; "></div>

                    <div class="login__box">
                        <input type="password" placeholder="Conform Password"
                            id="confirmPassword" class="login__input">
                        <i class="ri-lock-2-fill"></i>
                    </div>
                    <input type="hidden" name="referral" id="referralInput"
                        value>

                    <div class="error-message" id="confirm-error"
                        style="  display: none;
                        color: rgb(161 17 17);
                        font-weight: bold;
                        font-size: 12px; "></div>
                </div>

                <div class="login__check">
                    <% if(typeof message !=='undefined' ){ %>

                    <h6 class="text-danger">
                        <%= message %>
                    </h6>

                    <% } %>

                    <a href="#" class="login__forgot">Forgot Password?</a>
                </div>

                <button type="submit" id="submit"
                    class="login__button">SignUp</button>

                <div class="login__register">
                    Alredy have account?? <a href="login">login</a>
                </div>

            </form>

        </div>
    </div>
   
